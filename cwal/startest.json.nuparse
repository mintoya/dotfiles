format = '''
$directory$git_branch
$character
'''

palette = 'colors'

[palettes.colors]
foreground = '{foreground}'
%nu%{}%nu%
color0     = '{color0}'
color1     = '{color1}'
color2     = '{color2}'
color3     = '{color3}'
color4     = '{color4}'
color5     = '{color5}'
color6     = '{color6}'
color7     = '{color7}'
color8     = '{color8}'
color9     = '{color9}'

# Prompt symbols
[character]
# success_symbol = "[   >](color1)"
# vicmd_symbol   = "[   x](color1)"
# error_symbol   = "[   ?](red)"
success_symbol = "[  ](color1)"
vicmd_symbol   = "[  ](color1)"
error_symbol   = "[  ](red)"

[directory]
format = "[ ÓÇ∂](fg:color5 bg:none)[ÔÅª ](bg:color5 fg:foreground)[ÓÇ¥ ](fg:color5 bg:none)[$path ](fg:foreground bg:none)"


